{
  "name": "@eatonfyi/serializers",
  "version": "1.0.0",
  "description": "Shared tools for serializing and deserializing data in assorted formats.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll",
    "clean": "shx rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "author": "eaton",
  "license": "MIT",
  "devDependencies": {
    "@eatonfyi/fs-jetpack": "github:eaton/fs-jetpack",
    "@types/node": "^20.11.30",
    "ava": "^6.1.2",
    "pkgroll": "^2.0.2",
    "shx": "^0.3.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@sindresorhus/is": "^6.2.0",
    "@types/iarna__toml": "^2.0.5",
    "@types/ini": "^4.1.0",
    "@types/plist": "^3.0.5",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "entities": "^4.5.0",
    "gray-matter": "^4.0.3",
    "ini": "^4.1.2",
    "json5": "^2.2.3",
    "plist": "^3.1.0",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  }
}
